@model List<Variant>
@{
    ViewData["Title"] = "Home Page";
}

<a href="/home/calc/" class="btn btn-primary">Новый расчёт</a>

<table class="table table-hover">
    <tr>
        <th>Высота слоя, м</th>
        <th>Начальная температура материала, С</th>
        <th>Начальная температура газа, С</th>
        <th>Скорость газа на свободное сечение шахты, м/с</th>
        <th>Средняя теплоемкость газа, кДж/(м^3 * К)</th>
        <th>Расход материалов, кг/с</th>
        <th>Теплоемкость материалов, кДж/(кг * К)</th>
        <th>Объемный коэффициент теплоотдачи, Вт/(м^3 * К)</th>
        <th>Диаметр аппарата, м</th>
        <th>Действия</th>
    </tr>
    @foreach (var item in Model)
    { 
        <tr>
            <td>@item.Num1</td>
            <td>@item.Num2</td>
            <td>@item.Num3</td>
            <td>@item.Num4</td>
            <td>@item.Num5</td>
            <td>@item.Num6</td>
            <td>@item.Num7</td>
            <td>@item.Num8</td>
            <td>@item.Num9</td>
            <td>
                <a href="/home/table/@item.Id">Открыть</a> <a href="/home/delete/@item.Id">Удалить</a>
            </td>
        </tr>

    }

</table>
